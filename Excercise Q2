N = input() #26
flag = 0
N = int(N)
initial_N = N #26
while True:
    ten = N//10 # 2
    one = N % 10 # 6
    total = ten + one # 8
    right = total % 10 # 우측 숫자
    left = one # 좌측 숫자
    new = left*10 + right
    N = new
    flag = flag + 1
    if new == initial_N:
        print(f'{N}, {flag}')
        break
